{
  "name": "janos",
  "version": "1.0.0",
  "description": "The Janos Gat Gallery website",
  "engines": {
    "node": ">=6.3"
  },
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint && NODE_ENV=test npm run webpack",
    "test": "mocha --recursive spec/bootstrap.js spec",
    "preci": "NODE_ENV=test npm run webpack",
    "ci": "mocha --watch --recursive spec",
    "lint": "eslint *.js **/*.js *.jsx **/*.jsx",
    "clean": "rimraf src/assets/build",
    "prewebpack": "npm run clean",
    "webpack": "webpack --config config/build/webpack.config.js",
    "prewebpack-server": "npm run clean",
    "webpack-server": "webpack-dev-server --config config/build/webpack-dev-server.config.js",
    "build": "docker-compose -f config/deploy/docker-compose.yml build production",
    "docker": "docker-compose -f config/deploy/docker-compose.yml up production",
    "docker-webpack": "docker-compose -f config/deploy/docker-compose.yml up production-docker",
    "prestart": "NODE_ENV=production npm run webpack",
    "start": "NODE_ENV=production node index.js",
    "start-webpack": "NODE_ENV=production npm run webpack-server",
    "build-devel": "docker-compose -f config/deploy/docker-compose.yml build development",
    "docker-devel": "docker-compose -f config/deploy/docker-compose.yml up development",
    "docker-webpack-devel": "docker-compose -f config/deploy/docker-compose.yml up development-docker",
    "prestart-devel": "NODE_ENV=development npm run webpack",
    "start-devel": "NODE_ENV=development node index.js",
    "start-webpack-devel": "USE_HOT_SERVER=true NODE_ENV=development npm run webpack-server",
    "build-test": "docker-compose -f config/deploy/docker-compose.yml build test",
    "docker-test": "docker-compose -f config/deploy/docker-compose.yml up test",
    "docker-ci": "docker-compose -f config/deploy/docker-compose.yml run ci",
    "docker-webpack-test": "docker-compose -f config/deploy/docker-compose.yml up test-docker",
    "prestart-test": "NODE_ENV=test npm run webpack",
    "start-test": "NODE_ENV=test node index.js",
    "start-webpack-test": "NODE_ENV=test npm run webpack-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbrio/janos.git"
  },
  "author": "Michael Diolosa <michael.diolosa@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mbrio/janos/issues"
  },
  "homepage": "https://github.com/mbrio/janos#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "csswring": "^5.1.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-config-mbrio": "^1.0.3",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.1",
    "file-loader": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.2",
    "postcss-loader": "^0.10.1",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "superagent": "^2.2.0",
    "supertest": "^2.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "chokidar": "^1.6.0",
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "history": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "reselect": "^2.5.3",
    "rimraf": "^2.5.4",
    "seamless-immutable": "^6.1.1"
  }
}
